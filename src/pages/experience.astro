---
import BaseLayout from "../layouts/BaseLayout.astro";
import Grid from "../components/Grid.astro";
import Tag from "../components/Tag.astro";

import ContactCTA from "../components/ContactCTA.astro";
import Hero from "../components/Hero.astro";
---

<BaseLayout
  title="Experience | Logan Jorgensen"
  description="Learn about Logan Jorgensen's most recent projects"
>
  <div class="stack gap-20">
    <main class="wrapper stack gap-8 justify-content-center">
      <Hero
        title="Work Experience"
        tagline="See where I've worked and the path I've taken to get to where I am today."
        align="start"
      />
      <!-- List of previous employers with name of role and associated dates. They should each have a picture for a logo and centered on the page -->
      <!-- Grid for Work Experience -->
      <Grid>
        <!-- Capital One, Full Time -->
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/capitalone.webp"
                alt="Capital One"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">Capital One</h3>
              <p class="job-detail
              ">Associate Software Engineer</p>
              <p class="job-detail">August 2024 — Present</p>
              <p class="job-detail">Chicago, IL</p>
              <div class="card__tags
              ">
                <Tag>Go</Tag>
                <Tag>Python</Tag>
                <Tag>AWS</Tag>
              </div>
            </div>
          </div>
        </li>
        <!-- EveryNetNet -->
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/projects/everynetnetlogo.jpg"
                alt="EveryNetNet"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">EveryNetNet</h3>
              <p class="job-detail">Founder</p>
              <p class="job-detail">December 2023 — August 2024</p>
              <p class="job-detail">Ames, IA</p>
              <div class="card__tags">
                <Tag>React</Tag>
                <Tag>Google Cloud</Tag>
                <Tag>Puppeteer</Tag>
                <Tag>Stripe API</Tag>
              </div>
            </div>
          </div>
        </li>
        <!-- FloVision -->
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/flovision.jpeg"
                alt="FloVision"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">FloVision Solutions</h3>
              <p class="job-detail">Software Engineer Intern</p>
              <p class="job-detail">February 2024 — May 2024</p>
              <p class="job-detail">Ames, IA</p>
              <div class="card__tags">
                <Tag>Python</Tag>
                <Tag>OpenCV</Tag>
                <Tag>AWS</Tag>
              </div>
            </div>
          </div>
        </li>
        <!-- Iowa State University - Peer Mentor -->
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/iowastate.png"
                alt="Iowa State University"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">Iowa State University</h3>
              <p class="job-detail">Peer Mentor (Computer Science)</p>
              <p class="job-detail">August 2023 — March 2024</p>
              <p class="job-detail">Ames, IA</p>
              <div class="card__tags">
                <Tag>Git</Tag>
                <Tag>Mentoring</Tag>
                <Tag>Leadership</Tag>
              </div>
            </div>
          </div>
        </li>
        <!-- Capital One -->
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/capitalone.webp"
                alt="Capital One"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">Capital One</h3>
              <p class="job-detail">Software Engineer Intern</p>
              <p class="job-detail">June 2023 — August 2023</p>
              <p class="job-detail">New York, NY</p>
              <div class="card__tags">
                <Tag>Python</Tag>
                <Tag>AWS</Tag>
                <Tag>Jenkins</Tag>
                <Tag>Locust</Tag>
              </div>
            </div>
          </div>
        </li>
        <!-- Setwise Technology -->
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/setwisetechnology.png"
                alt="Setwise Technology"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">Setwise Technology</h3>
              <p class="job-detail">Software Engineer Intern</p>
              <p class="job-detail">May 2022 — October 2022</p>
              <p class="job-detail">Ames, IA</p>
              <div class="card__tags">
                <Tag>PHP</Tag>
                <Tag>Laravel</Tag>
                <Tag>Vue.js</Tag>
              </div>
            </div>
          </div>
        </li>
        <!-- Additional Experiences -->
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/capitalone.webp"
                alt="Capital One"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">Capital One</h3>
              <p class="job-detail">Software Engineer Intern</p>
              <p class="job-detail">June 2022 — August 2022</p>
              <p class="job-detail">McLean, VA</p>
              <div class="card__tags">
                <Tag>Python</Tag>
                <Tag>Go</Tag>
                <Tag>Git</Tag>
                <Tag>PostgreSQL</Tag>
                <Tag>FastAPI</Tag>
                <Tag>Lit</Tag>
              </div>
            </div>
          </div>
        </li>
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/iowastate.png"
                alt="Iowa State University"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">Iowa State University</h3>
              <p class="job-detail">IT Support Specialist</p>
              <p class="job-detail">November 2021 — June 2022</p>
              <p class="job-detail">Ames, IA</p>
              <div class="card__tags">
                <Tag>Troubleshooting</Tag>
                <Tag>Technical Communication</Tag>
                <Tag>Customer Service</Tag>
              </div>
            </div>
          </div>
        </li>
        <li>
          <div class="card">
            <div class="card__image">
              <img
                src="/assets/doordash.png"
                alt="DoorDash"
              />
            </div>
            <div class="card__content">
              <h3 class="card__title">DoorDash</h3>
              <p class="job-detail">Delivery Driver</p>
              <p class="job-detail">February 2021 — November 2021</p>
              <p class="job-detail">Ames, IA</p>
              <div class="card__tags">
                <Tag>Customer Service</Tag>
                <Tag>Time Management</Tag>
                <Tag>Problem Solving</Tag>
              </div>
            </div>
          </div>
